{
    "name": "Remove Verify Badge for Twitter",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "This is a browser extension that removes the verify badge on Twitter.",
    "icons": {
      "128": "icons/icon-128.png",
      "64":"icons/icon-64.png",
      "48":"icons/icon-48.png",
      "16":"icons/icon-16.png"
    },
    "content_scripts": [{
      "matches": ["*://twitter.com/*"],
      "js": [
        "index.js"
      ]
    }]
  }